<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<BorderPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
            <children>
                <Button fx:id="btn1" mnemonicParsing="false" text="Перевести гроші">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                    </VBox.margin></Button>
                <Button fx:id="btn3" mnemonicParsing="false" text="Провести оплату">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                    </VBox.margin></Button>
                <Button fx:id="btn4" mnemonicParsing="false" text="Показати баланс">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                    </VBox.margin></Button>
                <Button fx:id="btn5" mnemonicParsing="false" text="Зняти гроші">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                    </VBox.margin></Button>
                <Button fx:id="btn6" mnemonicParsing="false" text="Поповнити рахунок">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                    </VBox.margin></Button>
                <Button fx:id="btn7" mnemonicParsing="false" text="Профіль">
                    <VBox.margin>
                        <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
                    </VBox.margin></Button>
            </children>
            <VBox.margin>
                <Insets bottom="15.0" top="15.0" />
            </VBox.margin>
            <padding>
                <Insets bottom="15.0" top="15.0" />
            </padding>
            <opaqueInsets>
                <Insets left="15.0" top="15.0" />
            </opaqueInsets>
            <BorderPane.margin>
                <Insets />
            </BorderPane.margin>
        </VBox>
    </center>
</BorderPane>
